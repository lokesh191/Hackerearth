a=input()
k=0
for i in range(len(a)):
    if(a[i]=='l'):
        k+=1
        break
for j in range(i+1,len(a)):
    if(a[j]=='o'):
        k+=1
        break
for k in range(j+1,len(a)):
    if(a[k]=='v'):
        k+=1
        break
for s in range(k+1,len(a)):
    if(a[s]=='e'):
        k+=1
if(k==4):
   print("I love you, too!")
else:
    print("Let us breakup!")
